/////////////// Gird View List View Code=
<div id="myBtnContainer">
    <button class="btnr" onclick="listView()"><i class="fa fa-bars"></i> List</button> 
    <button class="btnr as" onclick="gridView()"><i class="fa fa-th-large"></i> Grid</button> 
</div>
<script>
    // Get the elements with class="any"
    var elements = document.getElementsByClassName("custom-media");
    var elements1 = document.getElementsByClassName("btnr");
    
    // Declare a loop variable
    var i;

    // List View
    function listView() {
      for (i = 0; i < elements.length; i++) {
        elements[i].classList.remove('gridclass');
        elements1[i].classList.toggle('as');
      }
    }

    // Grid View
    function gridView() {
      for (i = 0; i < elements.length; i++) {
         elements[i].classList.add('gridclass');
         elements1[i].classList.toggle('as');
      }
    }
</script>

